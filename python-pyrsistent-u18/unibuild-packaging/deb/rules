#!/usr/bin/make -f

# This file was automatically generated by stdeb 0.8.5 at
# Sat, 09 Feb 2019 12:28:42 +0100
export PYBUILD_NAME=pyrsistent

PYTHON3S:=$(shell py3versions -vr)

%:
	dh $@ --with python3 --buildsystem=pybuild
# python3 package build creates a dependency to python3 << 3.6
# and Ubuntu18 ships with python3.6 only, so no go
#	dh $@ --with python2,python3 --buildsystem=pybuild

override_dh_auto_install:
	dh_auto_install
	rm -rf debian/python-pyrsistent-u18/trash

override_dh_installchangelogs:
	dh_installchangelogs CHANGES.txt

override_dh_auto_test:

# Ubuntu .ddeb are currently not supported by reprepro, so we don't build them
override_dh_strip:
	dh_strip --no-automatic-dbgsym
